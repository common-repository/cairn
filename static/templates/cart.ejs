<div class="fineart-thecart">
	<h2 class="site-page-title"></h2>

	<% if ( cart['products'].length > 0 ) { %>

	<% if ( typeof(request) != 'undefined' ) { %>
		<% if ( request['success'] ) { %>
			<div id="messages" class="success"><%= request['message'] %></div>
		<% } else { %>
			<div id="messages"><%= request['message'] %></div>
		<% } %>
	<% } else { %>		
		<div id="messages">Please select a <strong>payment method</strong> and enter a <strong>zip code</strong> or country to calculate total.</div>
	<% } %>		

<div id="fineart-thecart-items">
<h3>Your Artwork</h3>

<% var total=0; %>
<% for(var i=0; i<cart['products'].length; i++) { %>
<% var option = cart['products'][i]['post']['options'][cart['products'][i]['option']]; %>

<div class="fineart-thecart-item" data-id="cart['products'][i]['id']" data-quantity="cart['products'][i]['quantity']" data-price="<%= option['price'] %>" data-option="<%= cart['products'][i]['option'] %>" data-weight="<%= option['weight'] %>" data-width="<%= option['width'] %>" data-length="<%= option['length'] %>" data-height="<%= option['height'] %>">

<div class="fineart-thecart-thumbnail">
<a href="<%= cart['products'][i]['post']['permalink'] %>"><img src="<%= cart['products'][i]['post']['image'][0] %>?w=75" /></a>
</div>

<div class="fineart-thecart-info">
<a href="<%= cart['products'][i]['post']['permalink'] %>"><%= cart['products'][i]['post']['title'] %></a>
<div class="fineart-thecart-dimensions"><%= cart['products'][i]['post']['details']['width'] %> x <%= cart['products'][i]['post']['details']['height'] %> inches</div>
<div class="fineart-thecart-medium"><%= cart['products'][i]['post']['details']['medium'] %></div>
<div><%= option['name'] %></div>
</div>

<div class="fineart-thecart-optioninfo">

<div><input data-id="<%= cart['products'][i]['id'] %>" data-option="<%= cart['products'][i]['option'] %>" class="quantity-field" type="text" value="<%= cart['products'][i]['quantity'] %>" size="2" /></div>
</div>

<span data-option="<%= cart['products'][i]['option'] %>" data-id="<%= cart['products'][i]['id'] %>" class="fineart-thecart-removeitem">X</span>

<br class="clearfix"/>

</div>
<% } %>

<div id="totals" class="fineart-thecart-total">
</div>

<span id="cart-back">Continue Shopping</span> 


</div>



